/*
1_1. ​Найти, на сколько нулей оканчивается n! = 1 * 2 * 3 * ... * n.
n ≤ 1000.
*/

#include <cstdio>
using  namespace std;

int count_of_zero(int input){
    int check = 5, result = 0;
    for(int i = 5; i <= input; i+=5){
        while(i % check == 0) {
            result++;
            check*=5;
        }
        check = 5;
    }
    return result;
}

int main()
{
    int input;
    scanf("%d", &input);
    printf("%d", count_of_zero(input));
    return 0;
}
